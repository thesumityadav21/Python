def bsearch(list1, key):
    l = 0
    r = len(list1) - 1

    while l <= r:
        m = 1 + r // 2

        if list1[m] < key:
            l = m + 1

        elif list1[m] > key:
            r = m - 1

        else:
            return m

list1 = [1,2,3,4,5,6,7,8]
key = 5
res = bsearch(list1, key)

if res == -1:
    print("not found")
else:
    print("fount at", res)

